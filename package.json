    "fs-extra": "^8.0.1",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "temp-filesystem": "^1.1.1",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "typescript-tslint-plugin": "^0.4.0"
  }
}
